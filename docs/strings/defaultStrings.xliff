                    </segment>
                </unit>
                <unit id="st_n_dec4">
                    <segment>
                        <source>Decrease 4</source>
                    </segment>
                </unit>
                <unit id="st_n_Tw2L">
                    <segment>
                        <source>Twist two stitches left</source>
                    </segment>
                </unit>
                <unit id="st_n_Tw2R">
                    <segment>
                        <source>Twist two stitches right</source>
                    </segment>
                </unit>
                <unit id="st_n_k-b">
                    <segment>
                        <source>Knit stitch in row below</source>
                    </segment>
                </unit>
                <unit id="st_n_p-b">
                    <segment>
                        <source>Purl stitch in row below</source>
                    </segment>
                </unit>
                <unit id="st_n_kfb">
                    <segment>
                        <source>Knit front and back</source>
                    </segment>
                </unit>
                <unit id="st_n_skl">
                    <segment>
                        <source>Slip, knit, lift</source>
                    </segment>
                </unit>
                <unit id="st_n_kll">
                    <segment>
                        <source>Knit left loop</source>
                    </segment>
                </unit>
                <unit id="st_n_M1PL">
                    <segment>
                        <source>Make one purl left</source>
                    </segment>
                </unit>
